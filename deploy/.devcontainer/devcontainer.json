{
	"name": "deploy",
	"dockerComposeFile": "${localWorkspaceFolder}/../docker-compose.yml",
	"service": "deploy",
	"workspaceFolder": "/app",
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker": "latest"
	},	
	"forwardPorts": [],
	"postCreateCommand": "",
	"customizations": {
		"vscode": {
			"extensions": [
				"github.copilot",
				"github.copilot-chat",
				"dbaeumer.vscode-eslint",
				"amazonwebservices.aws-toolkit-vscode"
			]
		}
	},
	"runServices": [
		"deploy"
	]
}